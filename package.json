{
  "name": "@appstorys/appstorys-react-native",
  "version": "1.1.2",
  "private": false,
  "author": "AppStorys",
  "license": "ISC",
  "description": "Official React Native SDK for integrating AppStorys services into a react native application.",
  "source": "./src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./babel-plugin": "./babel-plugin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean:dist": "rimraf dist",
    "build:bundle": "rollup -c",
    "obfuscate": "javascript-obfuscator dist/core.bundle.js --output dist/core.bundle.obf.js --compact true --control-flow-flattening true --control-flow-flattening-threshold 0.7 --string-array true --string-array-threshold 0.75 --self-defending true --disable-console-output true --reserved-names \"/^(?:AppStorys|Banner|Floater|Pip|Stories|StoriesScreen|PipScreen|Survey|Csat|Widgets|Modal|BottomSheet|CaptureScreenButton|Measurable|MeasurementProvider)$/\"",
    "build:dts": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --project tsconfig.json",
    "write:wrapper": "node scripts/writeWrapper.js",
    "prepare:dist": "npm run clean:dist && npm run build:bundle && npm run obfuscate && npm run build:dts && npm run write:wrapper",
    "prepublishOnly": "npm run prepare:dist",
    "test:pack": "npm pack"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "sdk",
    "appstorys",
    "appstorys-react-native"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/appversal/FinalRNSDK.git",
  "dependencies": {
    "events": "^3.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@octokit/plugin-paginate-rest": "^12.0.0",
    "@octokit/request": "^9.2.3",
    "@octokit/request-error": "^6.1.8",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.5",
    "@types/react": "^19.1.12",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "isolated-vm": "^5.0.4",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.1",
    "react-native-builder-bob": "^0.30.2",
    "react-native-device-info": "^14.0.4",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.0.0",
    "react-native-localize": "^3.5.2",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-svg": "^15.12.1",
    "react-native-video": "^6.0.0",
    "react-native-view-shot": "^4.0.3",
    "release-it": "^19.0.2",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.7.1",
    "turbo": "^1.10.7",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=2.1.0",
    "@react-native-community/netinfo": ">=11.4.1",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-device-info": ">=14.0.4",
    "react-native-encrypted-storage": ">=4.0.3",
    "react-native-fs": ">=2.20.0",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-localize": ">=3.5.2",
    "react-native-reanimated": ">=3.0.0",
    "react-native-safe-area-context": ">=5.3.0",
    "react-native-svg": ">=15.12.1",
    "react-native-video": ">=6.0.0",
    "react-native-view-shot": ">=4.0.3",
    "react-native-worklets-core": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-gesture-handler": {
      "optional": true
    },
    "react-native-reanimated": {
      "optional": true
    },
    "react-native-safe-area-context": {
      "optional": true
    },
    "react-native-worklets-core": {
      "optional": true
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "module-legacy",
    "languages": "kotlin-objc",
    "version": "0.41.2"
  }
}